---
import { Icon as AstroIcon } from 'astro-icon/components';
import { icons, type Icons } from '../Icons';
const sizes = {
    small: 18,
    normal: 25,
    big: 35,
};
interface Props {
    type: Icons,
    size?: keyof typeof sizes,
}
---
<AstroIcon name={icons[Astro.props.type]} size={sizes[Astro.props.size ?? 'normal']}/>